  "license": "MIT",
  "engines": {
    "node": "*"
  },
  "dependencies": {
    "vscode-languageserver": "^3.4.3"
  },
  "devDependencies": {
    "@types/node": "^6.0.88",
    "typescript": "^2.5.2"
